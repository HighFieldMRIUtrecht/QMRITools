Cell["FinalGrads", "ObjectName",
 CellID->283094597,ExpressionUUID->"7b9fe633-c12e-449f-95a0-5b3f34dbcea1"],

Cell[TextData[{
 Cell["   ", "ModInfo",ExpressionUUID->"8d2c1f5a-3aaa-4925-90b3-fe55598bfab4"],
 Cell[BoxData[
  RowBox[{
   ButtonBox["FinalGrads",
    BaseStyle->"Link",
    ButtonData->"paclet:QMRIToolsGradientTools/ref/FinalGrads"], "[", 
   RowBox[{
    StyleBox["grtxt", "TI"], ",", 
    RowBox[{"{", 
     RowBox[{
      StyleBox["int", "TI"], ",", 
      StyleBox["intn", "TI"]}], "}"}], ",", 
    RowBox[{"{", 
     RowBox[{
      StyleBox["rand", "TI"], ",", 
      StyleBox["order", "TI"]}], "}"}]}], "]"}]], "InlineFormula",
  ExpressionUUID->"aba0352f-f38d-4dfe-8e80-8dcc1d06fd7f"],
 "\[LineSeparator]finalizes the gradient txt file. ",
 Cell[BoxData[
  StyleBox["grtxt", "TI"]], "InlineFormula",ExpressionUUID->
  "74cb9b6e-503f-4aaf-b509-a0e53978b67b"],
 " is the output from the function ",
 Cell[BoxData[
  ButtonBox["ConvertGrads",
   BaseStyle->"Link",
   ButtonData->"paclet:QMRITools/ref/ConvertGrads"]], "InlineFormula",
  ExpressionUUID->"d5ca0199-c3d5-4e01-9d06-3597b80bb164"],
 ", which convert the grad to txt format. int is ",
 Cell[BoxData[
  ButtonBox["True",
   BaseStyle->"Link"]], "InlineFormula",ExpressionUUID->
  "e51981ab-f040-4639-be65-2301b67d1bb0"],
 " or False, if set to ",
 Cell[BoxData[
  ButtonBox["True",
   BaseStyle->"Link"]], "InlineFormula",ExpressionUUID->
  "19f688a6-5a5e-45b8-be03-2ef6c707be8b"],
 " it interleaves b=0 gradients every intn directions. rand indicates if the \
gradients need to be randomized, for this it uses the order which is the \
output of ",
 Cell[BoxData[
  ButtonBox["FindOrder",
   BaseStyle->"Link",
   ButtonData->"paclet:QMRITools/ref/FindOrder"]], "InlineFormula",
  ExpressionUUID->"32e65cd2-cd7b-48c5-ab17-8a35b5269023"],
 "."
}], "Usage",
