Cell["DeNoise", "ObjectName",
 CellID->652281008,ExpressionUUID->"d33ada09-0c89-44ab-86ce-7753492188bf"],

Cell[TextData[{
 Cell["   ", "ModInfo",ExpressionUUID->"ff47aeff-3635-486e-a974-1e6a00b99e57"],
 "DeNoise[data,sigma,filtersize] removes Rician noise with standard deviation \
\"sigma\" from the given dataset using a kernel with size \"filtersize\" a \
gaussian kernel.\nDeNoise[data,sigma,filtersize, Kernel->\"kerneltype\"] \
removes Rician noise with standard deviation \"sigma\" from the given dataset \
using a kernel with size \"filtersize\" and type \"kerneltype\".\n\nOutput is \
",
 Cell[BoxData[
  StyleBox["data", "TI"]], "InlineFormula",ExpressionUUID->
  "9560bebe-ed1c-4e81-8158-b3927e568609"],
 " denoised.\n\nDeNoise[] is based on DOI: 10.1109/TMI.2008.920609."
}], "Usage",
 CellChangeTimes->{3.8521381049887466`*^9},
 CellID->354884320,ExpressionUUID->"1e47bb7c-fd52-4c8f-9192-883c42530e7a"],

