Cell[TextData[{
 Cell["   ", "ModInfo",ExpressionUUID->"1d7a9901-1111-4def-a066-5acc5f19aef2"],
 Cell[BoxData[
  RowBox[{
   ButtonBox["SimEvolve",
    BaseStyle->"Link",
    ButtonData->"paclet:QMRIToolsJcouplingTools/ref/SimEvolve"], "[", 
   RowBox[{
    StyleBox["din", "TI"], ",", "H", ",", 
    StyleBox["t", "TI"]}], "]"}]], "InlineFormula",ExpressionUUID->
  "f63d389b-1eb1-4785-a89e-c500bc5d334e"],
