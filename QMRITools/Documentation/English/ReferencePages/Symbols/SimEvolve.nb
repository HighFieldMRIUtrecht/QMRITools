Cell["SimEvolve", "ObjectName",
 CellID->853983532,ExpressionUUID->"58a60644-e1be-4649-b509-b344be29dc14"],

Cell[TextData[{
 Cell["   ", "ModInfo",ExpressionUUID->"cd47e6ea-6d0b-4e2b-9757-ffdf66fde154"],
 Cell[BoxData[
  RowBox[{
   ButtonBox["SimEvolve",
    BaseStyle->"Link",
    ButtonData->"paclet:QMRIToolsJcouplingTools/ref/SimEvolve"], "[", 
   RowBox[{
    StyleBox["din", "TI"], ",", "H", ",", 
    StyleBox["t", "TI"]}], "]"}]], "InlineFormula",ExpressionUUID->
  "e154dc87-d9dc-4027-acc2-9cc95d4857a6"],
 "\[LineSeparator]evolves the spin system ",
 Cell[BoxData[
  StyleBox["din", "TI"]], "InlineFormula",ExpressionUUID->
  "14f89855-2273-400b-bc87-0dd54a78feb9"],
 " given the hamiltonian ",
 Cell[BoxData[
  StyleBox["H", "TI"]], "InlineFormula",ExpressionUUID->
  "688a8506-5702-4596-9dc2-4adc7ff51fd1"],
 " over a time ",
 Cell[BoxData[
  StyleBox["t", "TI"]], "InlineFormula",ExpressionUUID->
  "b3d2ca0a-b846-431d-a4d2-fdee56eb63dc"],
 ". ",
 Cell[BoxData[
  StyleBox["din", "TI"]], "InlineFormula",ExpressionUUID->
  "0c4d95dc-41d8-4153-9b51-4b5c725fa604"],
 " and ",
 Cell[BoxData[
  StyleBox["H", "TI"]], "InlineFormula",ExpressionUUID->
  "d11f0b8c-aa55-46de-b05c-fd4edac69ef6"],
 " are generated by ",
 Cell[BoxData[
  ButtonBox["SimHamiltonian",
   BaseStyle->"Link",
   ButtonData->"paclet:QMRITools/ref/SimHamiltonian"]], "InlineFormula",
  ExpressionUUID->"19943254-42fe-40ac-aae7-160f90aa6712"],
 ". The output is a new spinsystem dout."
}], "Usage",
