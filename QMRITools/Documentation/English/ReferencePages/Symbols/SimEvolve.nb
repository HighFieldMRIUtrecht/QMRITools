Cell["SimEvolve", "ObjectName",
 CellID->409260464,ExpressionUUID->"58da2380-a6f7-45e4-a864-806353565452"],

Cell[TextData[{
 Cell["   ", "ModInfo",ExpressionUUID->"e40776f7-5bfa-4752-997d-13846feee7e4"],
 Cell[BoxData[
  RowBox[{
   ButtonBox["SimEvolve",
    BaseStyle->"Link",
    ButtonData->"paclet:QMRIToolsJcouplingTools/ref/SimEvolve"], "[", 
   RowBox[{
    StyleBox["din", "TI"], ",", "H", ",", 
    StyleBox["t", "TI"]}], "]"}]], "InlineFormula",ExpressionUUID->
  "74f3168a-7c6a-4d12-8e88-04887ce97310"],
 "\[LineSeparator]evolves the spin system ",
 Cell[BoxData[
  StyleBox["din", "TI"]], "InlineFormula",ExpressionUUID->
  "1e2575ff-ec4d-44b3-8c5b-0dff8f01c923"],
 " given the hamiltonian ",
 Cell[BoxData[
  StyleBox["H", "TI"]], "InlineFormula",ExpressionUUID->
  "7601abfa-352a-42d1-97ce-8901c0e5a2f8"],
 " over a time ",
 Cell[BoxData[
  StyleBox["t", "TI"]], "InlineFormula",ExpressionUUID->
  "991d7ab1-25bb-4e95-ad07-cd1e44d49a21"],
 ". ",
 Cell[BoxData[
  StyleBox["din", "TI"]], "InlineFormula",ExpressionUUID->
  "89d76777-8dda-4c11-8d00-05612885475e"],
 " and ",
 Cell[BoxData[
  StyleBox["H", "TI"]], "InlineFormula",ExpressionUUID->
  "bcbfda9b-77de-4941-8fa8-82e9256bf263"],
 " are generated by ",
 Cell[BoxData[
  ButtonBox["SimHamiltonian",
   BaseStyle->"Link",
   ButtonData->"paclet:QMRITools/ref/SimHamiltonian"]], "InlineFormula",
  ExpressionUUID->"f5b8f304-5001-42d7-85fe-65a039b8af2b"],
 ". The output is a new spinsystem dout."
}], "Usage",
